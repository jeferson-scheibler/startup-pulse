<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size android:width="130dp" android:height="130dp"/>
            <solid android:color="@android:color/transparent"/>
            <stroke android:width="0dp" android:color="@android:color/transparent"/>
            <padding android:left="0dp" android:top="0dp" android:right="0dp" android:bottom="0dp"/>
        </shape>
    </item>
    <item android:bottom="4dp" android:top="4dp" android:left="4dp" android:right="4dp">
        <shape android:shape="oval">
            <solid android:color="#220052FF"/>
        </shape>
    </item>
</layer-list>
